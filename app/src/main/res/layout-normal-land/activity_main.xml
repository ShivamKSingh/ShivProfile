<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                             xmlns:app="http://schemas.android.com/apk/res-auto"
                                             xmlns:tools="http://schemas.android.com/tools"
                                             android:layout_width="match_parent"
                                             android:layout_height="match_parent">

    <ImageView
            android:layout_width="0dp"
            android:layout_height="100dp" app:srcCompat="@drawable/devslopesbanner"
            android:id="@+id/imageView25"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" android:scaleType="centerCrop"/>
    <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" app:srcCompat="@drawable/devslopesprofilelogo"
            android:id="@+id/Logo" android:layout_marginTop="80dp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="32dp"/>
    <TextView
            android:text="Devslopes"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView3" android:layout_marginTop="28dp"
            app:layout_constraintTop_toBottomOf="@+id/imageView25" app:layout_constraintStart_toEndOf="@+id/Logo"
            android:layout_marginStart="32dp" android:textSize="24sp"/>
    <ImageView
            android:layout_width="45dp"
            android:layout_height="45dp" app:srcCompat="@drawable/camerabutton"
            android:id="@+id/imageView27"
            app:layout_constraintStart_toEndOf="@+id/textView3" app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintEnd_toStartOf="@+id/imageView28" app:layout_constraintTop_toTopOf="@+id/imageView28"
            app:layout_constraintBottom_toBottomOf="@+id/imageView28"
            app:layout_constraintHorizontal_chainStyle="packed"/>
    <ImageView
            android:layout_width="45dp"
            android:layout_height="45dp" app:srcCompat="@drawable/favoritebutton"
            android:id="@+id/imageView28"
            app:layout_constraintStart_toEndOf="@+id/imageView27" app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toStartOf="@+id/imageView29" app:layout_constraintTop_toTopOf="@+id/imageView29"
            app:layout_constraintBottom_toBottomOf="@+id/imageView29" android:layout_marginEnd="32dp"
            android:layout_marginStart="32dp"/>
    <ImageView
            android:layout_width="45dp"
            android:layout_height="45dp" app:srcCompat="@drawable/inboxbutton"
            android:id="@+id/imageView29"
            app:layout_constraintStart_toEndOf="@+id/imageView28" app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginTop="28dp"
            app:layout_constraintTop_toBottomOf="@+id/imageView25" android:layout_marginEnd="32dp"/>
    <ImageView
            android:layout_width="150dp"
            android:layout_height="150dp" app:srcCompat="@drawable/dataimage"
            android:id="@+id/imageView30"
            android:scaleType="centerCrop" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.5" app:layout_constraintEnd_toStartOf="@+id/imageView31"
            android:layout_marginBottom="16dp" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_chainStyle="packed" android:layout_marginTop="32dp"
            app:layout_constraintTop_toBottomOf="@+id/Logo" android:layout_marginStart="8dp"/>
    <ImageView
            android:layout_width="150dp"
            android:layout_height="150dp" app:srcCompat="@drawable/imagesimage"
            android:id="@+id/imageView31"
            android:scaleType="centerCrop" app:layout_constraintStart_toEndOf="@+id/imageView30"
            app:layout_constraintHorizontal_bias="0.5" app:layout_constraintEnd_toStartOf="@+id/imageView32"
            app:layout_constraintTop_toTopOf="@+id/imageView30"
            app:layout_constraintBottom_toBottomOf="@+id/imageView30" android:layout_marginStart="16dp"/>
    <ImageView
            android:layout_width="150dp"
            android:layout_height="150dp" app:srcCompat="@drawable/videosimage"
            android:id="@+id/imageView32"
            android:scaleType="centerCrop" app:layout_constraintStart_toEndOf="@+id/imageView31"
            app:layout_constraintHorizontal_bias="0.5" app:layout_constraintEnd_toStartOf="@+id/imageView33"
            app:layout_constraintTop_toTopOf="@+id/imageView31"
            app:layout_constraintBottom_toBottomOf="@+id/imageView31" android:layout_marginStart="16dp"/>
    <ImageView
            android:layout_width="150dp"
            android:layout_height="150dp" app:srcCompat="@drawable/codeimage"
            android:id="@+id/imageView33"
            android:scaleType="centerCrop" app:layout_constraintStart_toEndOf="@+id/imageView32"
            app:layout_constraintHorizontal_bias="0.5" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@+id/imageView32"
            app:layout_constraintBottom_toBottomOf="@+id/imageView32" android:layout_marginStart="16dp"
            android:layout_marginEnd="8dp"/>
</android.support.constraint.ConstraintLayout>